{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "pages/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "css/*",
            "use": "@vercel/static"
        },
        {
            "src": "public/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/pages/chat.html"
        },
        {
            "src": "/(.*)",
            "dest": "/pages/$1"
        }
    ]
}