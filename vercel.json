{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/pages/accueil.html"
        },
        {
            "src": "/(.*)",
            "dest": "/pages/$1"
        }
    ]
}